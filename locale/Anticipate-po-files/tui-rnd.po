#: bin/tui-rnd:73
#, possible-sh-format
msgid ""
"\n"
"NAME\n"
"  $ME - Prints a random number within range (1-100 by without arguments)\n"
"\n"
"SYNOPSYS\n"
"  $ME [-h|--help] [--version] [-p|--perm COUNT [STARTVALUE]] [MAXVAL] "
"[MINVAL MAXVAL]\n"
"  \n"
"OPTIONS\n"
"  -h|--help\tShows help and exit with 99\n"
"  --version\tShows version infoand exit with 0\n"
"  \n"
"  -p|--perm COUNT\tPrint COUNT different random numbers\n"
" \n"
"EXAMPLE\n"
"\n"
"\t$ME\n"
"\t35\n"
"\t\n"
"\t$ME 15\n"
"\t14\n"
"\t\n"
"\t$ME 300 320\n"
"\t303\n"
"\t\n"
"\t$ME -p 5 200\n"
"\t203 204 201 202 200\n"
"\t\n"
"\t$ME -l 3 100 200\n"
"\t176 109 133\n"
"\t\n"
"\n"
"BUGS\n"
"  Report bugs to: <tui-support@nongnu.org>\n"
"  \n"
"  TUI - Homepage: <https://savannah.nongnu.org/projects/tui>\n"
"  \n"
"  General help using GNU software: <http://www.gnu.org/gethelp/>\n"
"\n"
"AUTHOR\n"
"  Simon Arjuna Erat, <http://linux-scripter.blogspot.ch>\n"
msgstr ""
