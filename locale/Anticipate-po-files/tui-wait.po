"NAME\n"
"  $ME - Wait some time\n"
"\n"
"SYNOPSYS\n"
"  $ME [-h|--help] [--version] TIME [\"Message to user\"]\n"
"  \n"
"OPTIONS\n"
"  -h|--help\tShows help and exit with 99\n"
"  --version\tShows version infoand exit with 0\n"
"\n"
"EXAMPLE\n"
"\n"
"\t$ME 1m \"Wait a minute\"\n"
"\t\n"
"\t$ME 1h \"Wait an hour\"\n"
"\n"
"BUGS\n"
